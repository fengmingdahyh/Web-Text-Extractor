{
  "manifest_version": 3,
  "name": "Web Text Extractor",
  "version": "1.0",
  "description": "Extract text from web pages with a beautiful interface",
  "permissions": [
    "activeTab",
    "scripting",
  "tabs"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}